<div class="container">
  <h1>Add Auction Item</h1>
  <div class="row small col s4 m4">
    <%= form_for @item do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :description %>
      <%= f.text_field :description %>
      <%= f.submit "Add", class:"btn yellow darken-4" %>
    <% end %>
  </div>
</div>

